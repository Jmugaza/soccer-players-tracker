<%- include('../partials/header') %>
<table id="list">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Position</th>
        </tr>
    </thead>
    <tbody>
        <% players.forEach(function(p) { %>
          <tr>
            <td><%= p.firstName %></td>
            <td><%= p.lastName %></td>
            <td><%= p.position %></td>
            <td><a href="/players/<%= p._id %>">DETAILS</a></td>
          </tr>
        <% }); %>
    </tbody>
    
</table>

<%- include('../partials/footer') %>